#!/bin/csh
# Merge images using fxymerge 
date
</data/sonic1/spike.3d.sm.data \
/home/stgpzli/OS6bin/      /fxymerge \
nl=50 \
ns=100 \
ntau=400 \
dz=4 \
jpfile=job.print.new_merge \
mlimit=8  \
diskhdr=/data/sonic1/junk.hdr_host1 \
diskxyt=/data/sonic1/junk.xyt_host1 \
diskxyt=/data/sonic1/junk.xyt_host2 \
diskxyt=/data/sonic1/junk.xyt_host3 \
diskxyt=/data/sonic1/junk.xyt_host4 \
diskxyt=/data/sonic1/junk.xyt_host5 \
>/data/sonic1/spike.3d.mig.sm.data.merge
#
echo 'merge_completed'
date

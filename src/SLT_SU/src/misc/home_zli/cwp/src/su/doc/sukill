string sdoc =
" 								\n"
" SUKILL - zero out traces					\n"
" 								\n"
" sukill <stdin >stdout min= count=1				\n"
" 								\n"
" Required parameters						\n"
" 	min= 		first trace to kill (one-based)		\n"
" 								\n"
" Optional parameters						\n"
" 	count= 1	number of traces to kill 		\n"
" 								\n"
;

1.3
log
@removed copyright statement, stray occurences of 'ushort'
@
text
@#include "par.h"

/* initargs - make command line args available to subroutines
 *
 * Synopsis:
 *	void initargs(argc, argv);
 *
 * Credit:
 *	CWP: Shuki, Jack
 *
 *
 */



void initargs(int argc, char **argv)
{
	xargc = argc; xargv = argv;
	return;
}
@


